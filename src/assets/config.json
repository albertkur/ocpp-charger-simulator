    },
    {
      "file": "abb.station-template.json",
      "numberOfStations": 0
    },
    {
      "file": "evlink.station-template.json",
      "numberOfStations": 1
    },
    {
      "file": "schneider.station-template.json",
      "numberOfStations": 0
    }
